{
  "name": "arcticlang",
  "version": "0.0.4",
  "description": "A programming language, made easy, for beginners.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "compile": "pkg -t node14-linux-x64,node14-macos-x64,node14-win-x64 --out-path build/arctic dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Arcticlang/Arctic.git"
  },
  "keywords": [
    "Programming-Language",
    "Language",
    "Programming",
    "Arctic",
    "Arcticlang"
  ],
  "author": "MiccDev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Arcticlang/Arctic/issues"
  },
  "homepage": "https://github.com/Arcticlang/Arctic#readme",
  "devDependencies": {
    "@types/node": "^18.14.1"
  },
  "bin": {
    "arctic": "./bin/arctic"
  },
  "preferGlobal": true,
  "pkg": {
    "targets": [ "node14-linux-x64", "node14-maxos-x64", "node19-win-x64" ]
  }
}
